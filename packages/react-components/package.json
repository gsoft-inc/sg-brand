{
    "name": "@orbit-ui/react-components",
    "version": "11.0.1",
    "description": "Sharegate Orbit UI React components",
    "repository": "https://github.com/gsoft-inc/sg-orbit.git",
    "author": "Groupe Sharegate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist",
        "!/dist/**/docs/*",
        "!/dist/**/tests/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "yarn watch",
        "prebuild": "yarn clean",
        "build": "yarn build:js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel src -d dist --config-file ./babel.config.js --copy-files --extensions .js,.jsx,.ts,.tsx ",
        "prewatch": "yarn clean",
        "watch": "run-p \"build --watch\"",
        "clean": "rimraf dist"
    },
    "peerDependencies": {
        "@orbit-ui/css-normalize": "*",
        "@orbit-ui/icons": "*",
        "@orbit-ui/semantic-ui-theme": "*",
        "@orbit-ui/tachyons": "*",
        "lodash": "*",
        "moment": "*",
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "*",
        "@popperjs/core": "2.5.4",
        "@reach/auto-id": "0.11.2",
        "prop-types": "*",
        "react-dates": "21.8.0",
        "react-is": "17.0.1",
        "react-moment-proptypes": "*",
        "react-popper": "2.2.4",
        "semantic-ui-react": "2.0.1",
        "use-debounce": "5.2.1"
    },
    "gitHead": "12c3852000398fe0e93b8ebc2548bcb026478bfa"
}
