{
    "name": "@orbit-ui/react-components",
    "version": "18.1.1",
    "description": "Sharegate Orbit UI React components",
    "repository": "https://github.com/gsoft-inc/sg-orbit.git",
    "author": "Groupe Sharegate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist",
        "!/dist/**/docs/*",
        "!/dist/**/tests/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "yarn watch",
        "prebuild": "yarn clean",
        "build": "yarn build:types && yarn build:js",
        "build:types": "tsc -p ./tsconfig.types.json",
        "build:js": "babel src -d dist --config-file ./babel.config.js --copy-files --extensions .js,.jsx,.ts,.tsx ",
        "prewatch": "yarn clean",
        "watch": "yarn build:js --watch",
        "clean": "rimraf dist"
    },
    "peerDependencies": {
        "@orbit-ui/css-normalize": "*",
        "@orbit-ui/icons": "*",
        "@orbit-ui/tachyons": "*",
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "*",
        "@popperjs/core": "2.9.2",
        "@reach/auto-id": "0.15.0",
        "react-is": "17.0.2",
        "text-mask-core": "5.1.2",
        "use-debounce": "6.0.1"
    },
    "gitHead": "4d7ad695b1605caf34c39464f1635023890da087"
}
