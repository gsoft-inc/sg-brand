{
    "name": "@orbit-ui/react",
    "version": "20.1.2",
    "description": "ShareGate orbit UI for React",
    "repository": "https://github.com/gsoft-inc/sg-orbit.git",
    "author": "Groupe ShareGate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "style": "dist/index.css",
    "files": [
        "/dist",
        "!/dist/**/docs/*",
        "!/dist/**/tests/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "setup": "yarn create-dist",
        "prebuild": "yarn clean",
        "build": "run-s build:*",
        "build:types": "tsc --build ./tsconfig.build.json",
        "build:js": "babel src -d dist --config-file ./babel.config.js --copy-files --extensions .ts,.tsx ",
        "clean": "run-p clear-dist clear-ts-cache",
        "prepublishOnly": "yarn copy-license-to-dist",
        "clear-dist": "node ../../scripts/clear-dir dist",
        "clear-ts-cache": "yarn rimraf *.tsbuildinfo",
        "create-dist": "node ../../scripts/create-dir dist",
        "copy-license-to-dist": "node ../../scripts/copy-root-license dist"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "@orbit-ui/icons": "9.0.0",
        "@orbit-ui/react-components": "18.1.2"
    },
    "gitHead": "4d7ad695b1605caf34c39464f1635023890da087"
}
