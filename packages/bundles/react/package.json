{
    "name": "@orbit-ui/react",
    "version": "1.1.0",
    "description": "ShareGate orbit UI for React",
    "repository": "https://github.com/gsoft-inc/sg-orbit.git",
    "author": "Groupe ShareGate inc.",
    "license": "Apache-2.0",
    "main": "index.css",
    "publishConfig": {
        "access": "public",
        "directory": "dist"
    },
    "scripts": {
        "prebuild": "yarn clean",
        "build": "yarn copy-to-dist",
        "clean": "rimraf dist",
        "prepublishOnly": "yarn copy-license-to-dist",
        "copy-to-dist": "node scripts/copy-to-dist",
        "copy-license-to-dist": "node ../../../scripts/copy-root-license dist"
    },
    "dependencies": {
        "@orbit-ui/css-normalize": "1.0.5",
        "@orbit-ui/fonts": "1.0.4",
        "@orbit-ui/foundation": "1.0.4",
        "@orbit-ui/icons": "3.0.1",
        "@orbit-ui/react-components": "1.2.4",
        "@orbit-ui/semantic-ui-theme": "1.2.0",
        "@orbit-ui/tachyons": "2.0.6",
        "lodash": "*",
        "semantic-ui-react": "0.88.1",
        "styled-jsx": "3.2.1"
    }
}
