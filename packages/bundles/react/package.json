{
    "name": "@orbit-ui/react",
    "version": "19.0.2",
    "description": "ShareGate orbit UI for React",
    "repository": "https://github.com/gsoft-inc/sg-orbit.git",
    "author": "Groupe ShareGate inc.",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public",
        "directory": "dist"
    },
    "scripts": {
        "prebuild": "yarn clean",
        "build": "yarn copy-to-dist",
        "clean": "rimraf dist",
        "prepublishOnly": "yarn copy-license-to-dist",
        "copy-to-dist": "node scripts/copy-to-dist",
        "copy-license-to-dist": "node ../../../scripts/copy-root-license dist"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "@orbit-ui/css-normalize": "4.0.0",
        "@orbit-ui/fonts": "4.0.0",
        "@orbit-ui/foundation": "7.0.0",
        "@orbit-ui/icons": "9.0.0",
        "@orbit-ui/react-components": "17.0.2",
        "@orbit-ui/tachyons": "7.0.0"
    }
}
