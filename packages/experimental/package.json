{
    "name": "@orbit-ui/experimental",
    "version": "1.3.2",
    "description": "Orbit experimental components",
    "repository": "https://github.com/gsoft-inc/sg-orbit.git",
    "author": "Groupe Sharegate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "style": "dist/index.css",
    "files": [
        "/dist",
        "!/dist/**/docs/*",
        "!/dist/**/tests/*",
        "index.css"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prebuild": "yarn clean && yarn create-dist",
        "build": "run-s build:*",
        "build:types": "tsc --build ./tsconfig.build.json",
        "build:js": "babel src -d dist --config-file ../../babel.config.js --copy-files --extensions .js,.jsx,.ts,.tsx ",
        "clean": "rimraf dist tsconfig.build.tsbuildinfo",
        "create-dist": "node ../../scripts/create-dir dist"
    },
    "peerDependencies": {
        "@sharegate/orbit-ui": ">=32.1.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "*"
    },
    "devDependencies": {
        "@sharegate/orbit-ui": "32.2.2"
    },
    "gitHead": "f8b5019bdc53f68abe27e931387aeec7f0747d85"
}
