/*******************************
        Site Overrides
*******************************/

@successDisabled: @success100;
@negativeDisabled: @alert100;

.ui.button,
.ui.buttons .button {
    min-height: var(--scale-hotel);
    display: inline-flex;
    border-radius: calc(var(--scale-hotel) / 2);
    align-items: center;
    justify-content: center;
    padding: @verticalPadding 0 (@verticalPadding + @shadowOffset);
}

/* Reset appaearance when button is used as an anchor */
a.ui.button {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
}

/* LAYOUT */
/* LAYOUT | Sizes */
/* LAYOUT | Height/Radius | Sizes | Micro */
.ui.button.micro,
.ui.button.with-icon.micro,
.ui.micro.buttons .button,
.ui.micro.with-icon.buttons .button {
    min-height: var(--scale-delta);
    border-radius: calc(var(--scale-delta) / 2);
}

/* Micro */
.ui.micro.buttons .button,
.ui.micro.buttons .or,
.ui.micro.button {
    font-size: 0.625rem;
    padding: 0.125rem 0 0.125rem;
    padding-left: var(--scale-bravo);
    padding-right: var(--scale-bravo)
}

/* LAYOUT | Height/Radius | Sizes | Mini */
.ui.button.mini,
.ui.button.with-icon.mini,
.ui.mini.buttons .button,
.ui.mini.with-icon.buttons .button {
    min-height: var(--scale-echo);
    border-radius: calc(var(--scale-echo) / 2);
}

/* Mini */
.ui.mini.buttons .button,
.ui.mini.buttons .or,
.ui.mini.button {
    font-size: 0.75rem;
    padding: 0.125rem 0 0.125rem;
    padding-left: var(--scale-bravo);
    padding-right: var(--scale-bravo)
}

/* LAYOUT | Height/Radius | Sizes | Tiny */
.ui.button.tiny,
.ui.button.with-icon.tiny,
.ui.tiny.buttons .button,
.ui.tiny.with-icon.buttons .button {
    min-height: var(--scale-foxtrot);
    border-radius: calc(var(--scale-foxtrot) / 2);
}

/* LAYOUT | Height/Radius | Sizes | Small */
.ui.button.small,
.ui.button.with-icon.small,
.ui.small.buttons .button,
.ui.small.with-icon.buttons .button {
    min-height: var(--scale-golf);
    border-radius: calc(var(--scale-golf) / 2);
}

/* LAYOUT | Height/Radius | Sizes | Large */
.ui.button.large,
.ui.button.with-icon.large,
.ui.large.buttons .button,
.ui.large.with-icon.buttons .button {
    min-height: var(--scale-india);
    border-radius: calc(var(--scale-india) / 2);
}

/* LAYOUT | Padding | Sizes | Tiny */
.ui.button.tiny,
.ui.tiny.buttons .button {
    padding-top: 0.125em;
    padding-bottom: 0.125em;
    padding-left: var(--scale-charlie);
    padding-right: var(--scale-charlie);
}

/* LAYOUT | Padding | Sizes | Small */
.ui.button.small,
.ui.small.buttons .button {
    padding-top: 0.375rem;
    padding-bottom: 0.375rem;
    padding-left: var(--scale-delta);
    padding-right: var(--scale-delta);
}

/* LAYOUT | Padding | Sizes | Medium */
.ui.button.medium,
.ui.medium.buttons .button {
    padding-left: var(--scale-echo);
    padding-right: var(--scale-echo);
}

/* LAYOUT | Padding | Sizes | Large */
.ui.button.large,
.ui.large.buttons .button {
    padding-top: 0.5rem;
    padding-bottom: 0.5rem;
    padding-left: var(--scale-foxtrot);
    padding-right: var(--scale-foxtrot);
}

/* LAYOUT | Circular */
/* LAYOUT | Circular/Default */
.ui.button.circular,
.ui.button.with-icon.circular,
.ui.button.with-icon.without-text.circular {
    padding: 0!important;
    font-size: @typeScale6;
    min-height: var(--scale-hotel);
    min-width: var(--scale-hotel);
}

.ui.button.circular.compact,
.ui.button.with-icon.circular.compact,
.ui.button.with-icon.without-text.circular.compact {
    font-size: @typeScale4;
}

/* LAYOUT | Circular/micro */
.ui.button.micro.circular,
.ui.button.with-icon.micro.circular {
    font-size: @typeScale9;
    min-height: var(--scale-delta);
    min-width: var(--scale-delta);
}

/* LAYOUT | Circular/Mini */
.ui.button.mini.circular,
.ui.button.with-icon.mini.circular {
    font-size: @typeScale9;
    min-height: var(--scale-echo);
    min-width: var(--scale-echo);
}

/* LAYOUT | Circular/Tiny */
.ui.button.tiny.circular,
.ui.button.with-icon.tiny.circular {
    font-size: @typeScale8;
    min-height: var(--scale-foxtrot);
    min-width: var(--scale-foxtrot);
}

/* LAYOUT | Circular/Tiny|Compact */
.ui.button.tiny.circular.compact,
.ui.button.with-icon.tiny.circular.compact {
