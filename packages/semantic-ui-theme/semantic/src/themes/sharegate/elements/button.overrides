.ui.button.with-icon.ghost.primary:focus .icon,
.ui.button.with-icon.ghost.primary.focus .icon,
.ui.buttons.ghost.primary .button.with-icon:hover .icon,
.ui.buttons.ghost.primary .button.with-icon.active .icon,
.ui.buttons.ghost.primary .button.with-icon:focus .icon,
.ui.buttons.ghost.primary .button.with-icon.focus .icon {
    fill: @primary500;
}

/* CONTENT | Icon | STATE | Active | SKIN | Negative/Ghost */
.ui.button.with-icon.ghost.negative:hover .icon,
.ui.button.with-icon.ghost.negative.active .icon,
.ui.button.with-icon.ghost.negative:focus .icon,
.ui.button.with-icon.ghost.negative.focus .icon,
.ui.buttons.ghost.negative .button.with-icon:hover .icon,
.ui.buttons.ghost.negative .button.with-icon.active .icon,
.ui.buttons.ghost.negative .button.with-icon:focus .icon,
.ui.buttons.ghost.negative .button.with-icon.focus .icon {
    fill: @alert500;
}

/* CONTENT | Icon | STATE | Active | SKIN | Negative/Basic */
.ui.button.with-icon.negative.basic:hover .icon,
.ui.button.with-icon.negative.basic.active .icon,
.ui.button.with-icon.negative.basic:focus .icon,
.ui.button.with-icon.negative.basic.focus .icon,
.ui.buttons.negative.basic .button.with-icon:hover .icon,
.ui.buttons.negative.basic .button.with-icon.active .icon,
.ui.buttons.negative.basic .button.with-icon:focus .icon,
.ui.buttons.negative.basic .button.with-icon.focus .icon {
    fill: @alert900;
}

/* CONTENT | Icon | STATE | Active | SKIN | Naked */
.ui.button.naked:hover .icon,
.ui.button.naked:focus .icon,
.ui.button.naked.focus .icon,
.ui.button.naked.active .icon {
    fill: initial;
}

/* CONTENT | Group */
.ui.basic.buttons {
    border: none;
}

/* No border on button basic group until I say so */
.ui.basic.buttons .button {
    background-color: @white!important;
    border: 1px solid transparent;
    box-shadow: none;
    border-bottom: 1px solid @cloud100;
    border-top: 1px solid @cloud100;
}

.ui.basic.buttons .button:hover,
.ui.basic.buttons .button:focus,
.ui.basic.buttons .button.focus {
    box-shadow: none;
    border: 1px solid @marine500;
    background-color: @white!important;
    color: @marine900!important;
}

.ui.basic.button:focus,
.ui.basic.button.focus {
    box-shadow: 0px 0px 0px 1px var(--cloud-200) inset, 0px 0px 0px 2px var(--primary-200) !important;
}

.ui.basic.primary.buttons .button:focus,
.ui.basic.primary.buttons .button.focus,
.ui.basic.primary.button:focus,
.ui.basic.primary.button.focus {
    box-shadow: 0px 0px 0px @basicColoredBorderSize @primaryColor inset, 0px 0px 0px 2px var(--primary-200) !important;
}

/* Gives the fist button a border in order to emulate a border on the whole group */
.ui.buttons.basic .button:first-child {
    border-left: 1px solid @cloud100;
}

.ui.buttons.basic .button:last-child {
    border-right: 1px solid @cloud100;
}

/* change style on hover/focus */
.ui.buttons.basic .button:hover:first-child,
.ui.buttons.basic .button:focus:first-child,
.ui.buttons.basic .button.focus:first-child {
    border-left: 1px solid @marine500;
}

.ui.buttons.basic .button:hover:last-child,
.ui.buttons.basic .button:focus:last-child,
.ui.buttons.basic .button.focus:last-child {
    border-right: 1px solid @marine500;
}

/* CONTENT | Group | Icon | Default */
.ui.buttons.with-icon .button {
    padding: 0.25rem 0.75rem;
}

/* Setting border radius for group of buttons */
/* LAYOUT | Sizes | Medium */
.ui.buttons .button:first-child {
    border-top-left-radius: calc(var(--scale-hotel) / 2);
    border-bottom-left-radius: calc(var(--scale-hotel) / 2);
}

.ui.buttons .button:last-child {
    border-top-right-radius: calc(var(--scale-hotel) / 2);
    border-bottom-right-radius: calc(var(--scale-hotel) / 2);
}

/* LAYOUT | Sizes | Tiny */
.ui.buttons.tiny .button:first-child {
    border-top-left-radius: calc(var(--scale-foxtrot) / 2);
    border-bottom-left-radius: calc(var(--scale-foxtrot) / 2);
}

.ui.buttons.tiny .button:last-child {
    border-top-right-radius: calc(var(--scale-foxtrot) / 2);
    border-bottom-right-radius: calc(var(--scale-foxtrot) / 2);
}

/* LAYOUT | Sizes | Small */
.ui.buttons.small .button:first-child {
    border-top-left-radius: calc(var(--scale-golf) / 2);
    border-bottom-left-radius: calc(var(--scale-golf) / 2);
}

.ui.buttons.small .button:last-child {
    border-top-right-radius: calc(var(--scale-golf) / 2);
    border-bottom-right-radius: calc(var(--scale-golf) / 2);
}

/* LAYOUT | Sizes | Large */
.ui.buttons.large .button:first-child {
    border-top-left-radius: calc(var(--scale-india) / 2);
    border-bottom-left-radius: calc(var(--scale-india) / 2);
}

.ui.buttons.large .button:last-child {
    border-top-right-radius: calc(var(--scale-india) / 2);
    border-bottom-right-radius: calc(var(--scale-india) / 2);
}

/* LAYOUT | Sizes | Large */
.ui.buttons.large .button:first-child {
    border-top-left-radius: calc(var(--scale-india) / 2);
    border-bottom-left-radius: calc(var(--scale-india) / 2);
}

.ui.buttons.large .button:last-child {
    border-top-right-radius: calc(var(--scale-india) / 2);
    border-bottom-right-radius: calc(var(--scale-india) / 2);
}

/* CONTENT / STATE / Loading */
/* Specificity hack */
.ui.loading.loading.loading.loading.loading.loading.button .icon {
    fill: @transparent;
}

.ui.loading.button:before,
.ui.loading.button:after {
    margin: 0;
    transform: translate(-50%, -50%);
}

/* SKIN | Link */
.ui.button.link {
    background: none;
    color: var(--primary-500);
    font-weight: 400;
    padding: 0;
    min-height: auto;
    border-radius: 2px;
}

/* SKIN | Link | STATE | Active */
.ui.button.link:focus,
.ui.button.link.focus,
.ui.button.link:hover,
.ui.button.link:active {
    text-decoration: underline;
}

.ui.button.link:active,
.ui.button.link.active {
    color: var(--primary-900);
    box-shadow: none;
    background: none;
    text-decoration: underline;
}

.ui.button.link:focus,
.ui.button.link.focus {
    background-color: var(--primary-50);
    text-decoration: none;
    box-shadow: none!important;
}

.ui.button.link.basic:focus,
.ui.button.link.basic.focus {
    box-shadow: none!important;
}

.ui.button.link.active:hover {
    color: var(--primary-900);
    background: none;
    text-decoration: underline;
}

.ui.button.link.active:focus,
.ui.button.link.active.focus {
    color: var(--primary-900);
    background-color: var(--primary-50);
    text-decoration: underline;
}

.ui.button.link:focus:hover,
.ui.button.link.focus:hover {
    text-decoration: underline;
    background: var(--primary-50);
}

.ui.button.link.active:hover {
    text-decoration: underline;
}

.ui.button.link:active:hover:focus,
.ui.button.link:active:hover.focus {
    background: var(--primary-50);
}

.ui.button.link.basic:focus:hover,
.ui.button.link.basic.focus:hover {
    text-decoration: underline;
    box-shadow: none!important;
    background: var(--marine-50) !important;
}

/* SKIN | Link | Basic */
.ui.button.link.basic {
    color: var(--marine-500);
    box-shadow: none !important;
}

.ui.button.link.basic:active {
    color: var(--marine-900)!important;
}

.ui.button.link.basic:focus,
.ui.button.link.basic.focus {
    background-color: var(--marine-50)!important;
}

.ui.button.link.basic:active {
    background-color: var(--white)!important;
}

/* Animation */
@keyframes button-spin {
    from {
        transform: translate(-50%, -50%) rotate(0deg);
    }
    to {
        transform: translate(-50%, -50%) rotate(360deg);
    }
}
