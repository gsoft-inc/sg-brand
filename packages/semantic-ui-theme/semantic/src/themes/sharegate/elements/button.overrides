/* LAYOUT | Circular/Small */
.ui.button.small.circular,
.ui.button.with-icon.small.circular {
    font-size: @typeScale7;
    min-height: var(--scale-golf);
    min-width: var(--scale-golf);
}

/* LAYOUT | Circular/Small|Compact */
.ui.button.small.circular.compact,
.ui.button.with-icon.small.circular.compact {
