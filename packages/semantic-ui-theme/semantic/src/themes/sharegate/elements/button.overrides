.ui.button.naked.basic:disabled,
.ui.buttons.naked.basic .button.disabled,
.ui.buttons.naked.basic .button:disabled {
    background-color: @transparent!important;
    color: initial !important;
    box-shadow: 0px 0px 0px 1px transparent inset!important;
}

/* LAYOUT | Compact */
/* CONTENT | Compact | Layout | Padding */
.ui.button.compact.tiny,
.ui.tiny.buttons .button.compact {
    padding-left: calc(var(--scale-charlie) / 2);
    padding-right: calc(var(--scale-charlie) / 2);
}

/* CONTENT | Compact | Layout | Padding | Sizes | Small */
.ui.button.compact.small,
.ui.small.buttons .button.compact {
    padding-left: calc(var(--scale-delta) / 2);
    padding-right: calc(var(--scale-delta) / 2);
}

/* CONTENT | Compact | Layout | Padding | Sizes | Medium */
.ui.button.compact.medium,
.ui.medium.buttons .button.compact {
    padding-left: calc(var(--scale-echo) / 2);
    padding-right: calc(var(--scale-echo) / 2);
}

/* CONTENT | Compact | Layout | Padding | Sizes | Large */
.ui.button.compact.large,
.ui.large.buttons .button.compact {
    padding-left: calc(var(--scale-foxtrot) / 2);
    padding-right: calc(var(--scale-foxtrot) / 2);
}

.ui.button:focus,
.ui.button.focus,
.ui.buttons .button:focus,
.ui.buttons .button.focus, {
    box-shadow: 0px 0px 0px 2px var(--primary-200) !important;
}

.ui.button:focus,
.ui.button.focus {
    background-color: var(--marine-500);
}

.ui.button:focus:active,
.ui.button.focus:active,
.ui.button:focus:hover,
.ui.button.focus:hover {
    background-color: var(--marine-900);
}

.ui.button.primary:focus:active,
.ui.button.primary.focus:active,
.ui.button.primary:focus:hover,
.ui.button.primary.focus:hover {
    background-color: var(--primary-900);
}

.ui.button.naked:active {
    box-shadow: none;
}

/* STATE | Active | SKIN | Basic */
.ui.button.basic:hover,
.ui.button.basic.active,
.ui.button.basic:focus:hover,
.ui.button.basic.focus:hover {
    background-color: @white!important;
