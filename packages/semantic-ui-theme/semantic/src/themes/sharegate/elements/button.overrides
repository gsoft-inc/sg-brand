}

/* LAYOUT | Circular/Large */
.ui.button.large.circular,
.ui.button.with-icon.large.circular {
    font-size: @typeScale6;
    min-height: var(--scale-india);
    min-width: var(--scale-india);
}

/* LAYOUT | Circular/Large|Compact */
.ui.button.large.circular.compact,
.ui.button.with-icon.large.circular.compact {
    font-size: @typeScale4;
}

/* LAYOUT | Naked */
.ui.button.naked {
    background-color: @transparent;
    color: initial;
    box-shadow: 0px 0px 0px 1px transparent inset;
}

/* LAYOUT | Naked/Basic */
.ui.button.basic.naked,
.ui.button.naked.basic,
.ui.buttons.naked.basic .button,
.ui.buttons.naked.basic .button {
    background-color: @transparent!important;
}

/* LAYOUT | Naked | STATE | Pseudo */
.ui.button.naked:hover,
.ui.button.naked:focus,
.ui.button.naked.focus,
.ui.button.naked.active {
    background-color: @transparent;
    color: initial;
    box-shadow: 0px 0px 0px 1px transparent inset;
}

/* LAYOUT | Naked/Ghost | STATE | Pseudo */
.ui.button.naked.ghost:hover,
.ui.button.naked.ghost:focus,
.ui.button.naked.ghost.focus,
.ui.button.naked.ghost.active {
    background-color: @transparent;
    color: initial;
    box-shadow: 0px 0px 0px 1px transparent inset;
}

/* LAYOUT | Naked | STATE | Disabled */
.ui.button.disabled.naked,
.ui.button.naked:disabled,
.ui.buttons.naked .button.disabled,
.ui.buttons.naked .button:disabled {
