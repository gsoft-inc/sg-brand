{
    "npmClient": "yarn",
    "useWorkspaces": true,
    "version": null,
    "command": {
        "version": {
            "ignoreChanges": [
                "**/*.md*",
                "**/*.json",
                "**/*.config.js",
                "**/*rc*",
                "**/stories/**",
                "**/tests/**",
                "**/docs/**",
                "**/dist/**",
                "**/scripts/**"
            ],
            "allowBranch": [
                "master"
            ]
        }
    }
}
